{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }],
    "import/prefer-default-export": "off",
    "react/state-in-constructor": ["off", "never"],
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-async-promise-executor": ["off"]
  },
  "globals": {
    "localStorage": true,
    "fetch": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  }
}
